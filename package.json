{
  "name": "{{projectName}}",
  "version": "{{projectVersion}}",
  "description": "{{projectDescription}}",
  "main": "index.js",
  "scripts": {
    "debug-server": "nodemon --watch services debug/server/server.js",
    "debug-page": "run-p build-debug-page debug-server",
    "build": "webpack --config webpack.config.production.js",
    "build-debug-page": "webpack --config webpack.config.development.js",
    "lint": "eslint --fix --ext .js services/",
    "test": "jest --no-cache"
  },
  "author": "{{projectAuthor}}",
  "license": "ISC",
  "dependencies": {
    "app-module-path": "^2.1.0",
    "axios": "^0.17.1",
    "react": "^16.3.1",
    "react-dom": "^16.3.1",
    "react-json-view": "^1.19.1",
    "bluebird": "^3.4.1",
    "jest-cli": "^17.0.3"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-loader": "^7.1.3",
    "babel-preset-env": "^1.3.3",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.5.0",
    "css-loader": "^0.28.0",
    "eslint": "^5.0.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-node": "^8.0.1",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-standard": "^4.0.0",
    "extract-text-webpack-plugin": "^v4.0.0-alpha.0",
