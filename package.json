    "koa-bodyparser": "^3.2.0",
    "koa-nunjucks-2": "^3.0.0-alpha.5",
    "koa-router": "^7.0.1",
    "koa-static": "^4.0.2",
    "less": "^2.7.1",
    "less-loader": "^4.0.3",
    "nodemon": "^1.9.2",
    "npm-run-all": "^4.1.2",
    "style-loader": "^0.16.1",
    "webpack": "^4.14.0",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.10"
  },
  "jest": {
    "verbose": true,
    "notify": true,
    "collectCoverage": true,
    "testEnvironment": "node",
    "modulePaths": [
      "<rootDir>/services"
